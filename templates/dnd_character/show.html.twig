{% extends 'layout/base.html.twig' %}

{% block title %}DndCharacter{% endblock %}

{% block body %}
    <h1>DndCharacter</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ dnd_character.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ dnd_character.name }}</td>
            </tr>
            <tr>
                <th>Level</th>
                <td>{{ dnd_character.level }}</td>
            </tr>
            <tr>
                <th>Experience_points</th>
                <td>{{ dnd_character.experiencePoints }}</td>
            </tr>
            <tr>
                <th>Money</th>
                <td>{{ dnd_character.money }}</td>
            </tr>
            <tr>
                <th>Strength</th>
                <td>{{ dnd_character.strength }}</td>
            </tr>
            <tr>
                <th>Dexterity</th>
                <td>{{ dnd_character.dexterity }}</td>
            </tr>
            <tr>
                <th>Constitution</th>
                <td>{{ dnd_character.constitution }}</td>
            </tr>
            <tr>
                <th>Intelligence</th>
                <td>{{ dnd_character.intelligence }}</td>
            </tr>
            <tr>
                <th>Wisdom</th>
                <td>{{ dnd_character.wisdom }}</td>
            </tr>
            <tr>
                <th>Charisma</th>
                <td>{{ dnd_character.charisma }}</td>
            </tr>
            <tr>
                <th>Armor_class</th>
                <td>{{ dnd_character.armorClass }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('dnd_character_index') }}">back to list</a>

    <a href="{{ path('dnd_character_edit', {'id': dnd_character.id}) }}">edit</a>

    {{ include('dnd_character/_delete_form.html.twig') }}
{% endblock %}
